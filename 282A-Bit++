#include <iostream>
using namespace std;
int main(){
int n;
cin>>n;
int x=0;
string o;
for(int i=0;i<n;i++){
    cin>>o;
    if(o=="++X"){
        ++x;
    }
    else if(o=="--X"){
        --x;
    }
    else if(o=="X++"){
        x++;
    }
    else if(o=="X--"){
        x--;
    }    
}
cout<<x;
return 0;
}
